[alias]
   lg = log --graph --pretty=format:'%Cred%h%Creset %Cblue(%aN)%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative
   chp = cherry-pick
   st = status
   co = checkout
   merge = merge --no-ff
   pull = pull --no-ff
   news = !git lg $(git log --author='Tim Felgentreff' --pretty=format:%H -n1)..
[user]
   name = Tim Felgentreff
   email = "timfelgentreff@gmail.com"
[color]
   branch = auto
   diff = auto
   grep = auto
   interactive = auto
   ui = auto
[help]
   autocorrect = 1
[push]
   default = matching
[core]
   autocrlf = false
   excludesfile = /home/tim/.gitignore
   mergetool = meld
[gc]
   auto = 0
[lighthouse]
   account = redcar
   token = 4a93ef05ea961b24cb7c30890ea3caf0629b1223
   project = 25090
[diff]
   ignoreSubmodules = untracked
[merge]
   conflictstyle = diff3

